    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch via NPM",
            "runtimeVersion": "16",
            "runtimeExecutable": "npx",
            "runtimeArgs": [
                "expo",
                "start"
            ],
        },
        {
            "name": "Attach to packager",
            "cwd": "${workspaceFolder}",
            "type": "reactnative",
            "request": "attach",
            "port": 19000,
            "sourceMaps": true
        },
    ]
}
