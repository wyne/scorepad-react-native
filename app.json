    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wyne.scorepad",
      "buildNumber": "0.0.1",
      "splash": {
        "tabletImage": "./assets/splash-ipad.png",
        "resizeMode": "cover"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.wyne.scorepad",
      "versionCode": 3,
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "",
    "githubUrl": "https://github.com/wyne/keepscore"
  }
}